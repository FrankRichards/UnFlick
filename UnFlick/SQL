drop database FLICKRDB;
create database FLICKRDB;

Images table fields

CREATE TABLE config (
	key varchar primary key,
	value varchar
);

CREATE TABLE image (
	
	id int primary key,
	owner char(12),
	secret char(10),
	server int,
	farm int,
	title varchar(120),
	url_o varchar(120),
	height_o int,
	width_o int,
	date datetime,
	upload int,
	description clob,
	image blob
);

